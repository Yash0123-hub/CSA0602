#include <stdio.h>

void selectionSort(int arr[], int n) {
    int i, j, min, temp;
    for (i = 0; i < n - 1; i++) {
        min = i;
        for (j = i + 1; j < n; j++)
            if (arr[j] < arr[min])
                min = j;
        temp = arr[i];
        arr[i] = arr[min];
        arr[min] = temp;
    }
}

void printArray(int arr[], int n) {
    printf("[");
    for (int i = 0; i < n; i++) {
        printf("%d", arr[i]);
        if (i < n - 1) printf(", ");
    }
    printf("]\n");
}

int main() {
    int arr1[] = {5, 2, 9, 1, 5, 6};
    int n1 = 6;
    selectionSort(arr1, n1);
    printf("Sorted Random Array: ");
    printArray(arr1, n1);

    int arr2[] = {10, 8, 6, 4, 2};
    int n2 = 5;
    selectionSort(arr2, n2);
    printf("Sorted Reverse Array: ");
    printArray(arr2, n2);

    int arr3[] = {1, 2, 3, 4, 5};
    int n3 = 5;
    selectionSort(arr3, n3);
    printf("Sorted Already Sorted Array: ");
    printArray(arr3, n3);

    return 0;
}
