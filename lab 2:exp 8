`#include <stdio.h>
#include <string.h>

int main() {
    char *words[] = {"mass", "as", "hero", "superhero"};
    int n = 4;

    printf("Output: [");
    int first = 1;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i != j && strstr(words[j], words[i]) != NULL) {
                if (!first) printf(", ");
                printf("\"%s\"", words[i]);
                first = 0;
                break;
            }
        }
    }

    printf("]\n");
    return 0;
}

