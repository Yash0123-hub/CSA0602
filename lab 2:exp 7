#include <stdio.h>
#include <string.h>

int strStr(char haystack[], char needle[]) {
    int n = strlen(haystack);
    int m = strlen(needle);

    for (int i = 0; i <= n - m; i++) {
        if (strncmp(&haystack[i], needle, m) == 0) {
            return i;
        }
    }
    return -1;
}

int main() {
    char haystack[] = "sadbutsad";
    char needle[] = "sad";

    int result = strStr(haystack, needle);
    printf("Output: %d\n", result);

    return 0;
}
