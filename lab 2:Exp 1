#include <stdio.h>

// Function to print list (array)
void printList(int arr[], int size) {
    printf("[");
    for (int i = 0; i < size; i++) {
        printf("%d", arr[i]);
        if (i < size - 1)
            printf(", ");
    }
    printf("]\n");
}

// Function to sort list (for negative numbers case)
void sortList(int arr[], int size) {
    int temp;
    for (int i = 0; i < size - 1; i++) {
        for (int j = i + 1; j < size; j++) {
            if (arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
}

int main() {
    // 1. Empty list
    int list1[] = {};
    int size1 = 0;
    printf("Test Case 1: ");
    printList(list1, size1);

    // 2. List with one element
    int list2[] = {1};
    int size2 = 1;
    printf("Test Case 2: ");
    printList(list2, size2);

    // 3. List with all identical elements
    int list3[] = {7, 7, 7, 7};
    int size3 = 4;
    printf("Test Case 3: ");
    printList(list3, size3);

    // 4. List with negative numbers (sorted)
    int list4[] = {-5, -1, -3, -2, -4};
    int size4 = 5;
    sortList(list4, size4);
    printf("Test Case 4: ");
    printList(list4, size4);

    return 0;
}
